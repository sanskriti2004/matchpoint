{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.12",
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "SENTENCE_TRANSFORMERS_HOME": "/tmp/sentence_transformers"
  }
}
